{
  "IdentityOptions": {
    "SignIn": {
      "RequireConfirmedEmail": true
    },
    "Password": {
      "RequiredLength": 8,
      "RequiredUniqueChars": 1,
      "RequireDigit": true,
      "RequireUppercase": true,
      "RequireLowercase": false,
      "RequireNonAlphanumeric": true
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=.\\SQLEXPRESS;Database=MotoBest;Integrated Security=True;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
